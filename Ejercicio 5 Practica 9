#include<stdlib.h>
#include<stdio.h>
int main()
{
int a[10][4]={{1,0,0,0},
                      {1,2,0,0},
                      {1,2,3,0},
                      {1,2,3,4},
                      {1,2,3,0},
                      {1,2,0,0},
                      {1,0,0,0}};
int i,j;
 
 for(i=0;i<=6;i++)
{
for(j=0;j<=3;j++)
{
if(a[i][j]==0)
{
	printf(" ");
}
else
{
printf("\t%d",a[i][j]);
}
}
printf("\n");
}
}
